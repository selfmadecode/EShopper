@model IEnumerable<Product>

<h1>All Products</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Amount</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @if (!Model.Any())
        {

        }
        else
        {
            foreach (var product in Model)
            {
                <tr>
                    <td>@product.Name</td>
                    <td>@product.Amount</td>
                    <td>@product.Quantity</td>
                </tr>
            }
        }
    </tbody>
</table>

<button>Assign to Suppervisor</button>